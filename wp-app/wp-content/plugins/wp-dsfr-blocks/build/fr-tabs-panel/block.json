{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dsfr/fr-tabs-panel",
  "version": "1.0.2",
  "title": "Contenu d'onglet",
  "category": "widgets",
  "icon": "layout",
  "description": "Bloc DSFR contenu onglet.",
  "parent": [
    "dsfr/fr-tabs"
  ],
  "supports": {
    "html": false,
    "anchor": false,
    "align": false,
    "color": {
      "background": true,
      "text": true
    },
    "lock": false,
    "reusable": false
  },
  "attributes": {
    "index": {
      "type": "integer",
      "default": 0
    },
    "labelledby": {
      "type": "string",
      "default": ""
    },
    "id": {
      "type": "string",
      "default": ""
    }
  },
  "usesContext": [
    "dsfr/fr-tabs--id",
    "dsfr/fr-tabs--active-tab-index"
  ],
  "textdomain": "wp-dsfr-blocks",
  "editorScript": "file:./index.js"
}