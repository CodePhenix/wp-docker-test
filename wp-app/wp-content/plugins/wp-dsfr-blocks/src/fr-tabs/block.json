{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "dsfr/fr-tabs",
	"version": "1.0.2",
	"title": "Onglets",
	"category": "widgets",
	"icon": "table-row-after",
	"description": "Bloc DSFR onglets.",
	"supports": {
		"html": false,
		"anchor": true,
		"align": [ "wide", "full" ]
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"activeTabIndex": {
			"type": "integer",
			"default": 0
		}
	},
	"providesContext": {
		"dsfr/fr-tabs--active-tab-index": "activeTabIndex",
		"dsfr/fr-tabs--id": "id"
	},
	"textdomain": "wp-dsfr-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
