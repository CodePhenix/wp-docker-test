{
  "name": "wp-dsfr-theme",
  "version": "1.0.0",
  "description": "DSFR theme for WordPress",
  "author": "Be API",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeAPI/wp-dsfr"
  },
  "scripts": {
    "start": "npm run dsfr && webpack --watch --config config/webpack.dev.js",
    "build": "npm run dsfr && webpack --config config/webpack.prod.js",
    "bundle-report": "webpack --config config/webpack.prod.js --json=dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "image": "node config/image-sizes.js",
    "dsfr": "node config/dsfr.js",
    "theme-zip": "wp-scripts plugin-zip"
  },
  "files": [
    "assets",
    "components",
    "dist",
    "dist-dsfr",
    "inc",
    "languages",
    "patterns",
    "templates",
    "404.php",
    "archive.php",
    "autoload.php",
    "footer.php",
    "front-page.php",
    "functions.php",
    "header.php",
    "home.php",
    "index.php",
    "page.php",
    "screenshot.png",
    "search.php",
    "searchform.php",
    "single.php",
    "style.css"
  ],
  "license": "GPL-2.0",
  "dependencies": {
    "@gouvfr/dsfr": "1.13.0",
    "oneloop.js": "^5.0.2"
  },
  "devDependencies": {
    "@wordpress/blocks": "^14.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.0.0",
    "@wordpress/dom-ready": "^3.17.0",
    "@wordpress/env": "^9.8.0",
